{
  "name": "sedasmanager",
  "productName": "SEDAS-manager",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "author": "Daniel Pojhan <danpojhan@gmail.com>",
  "homepage": "https://github.com/HelloWorld7894/SEDAC",
  "license": "GPL-3.0",
  "scripts": {
    "debug": "electron --inspect=9229 ./src/main.js",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build_depr": {
    "appId": "com.sedacmanager.app",
    "directories": {
      "output": "./src_build/",
      "buildResources": "resources"
    },
    "extraResources": [
      {
        "from": "./res/img/",
        "to": "img"
      },
      {
        "from": "./res/data/",
        "to": "data"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "./res/img/sedas-manager-logo.ico"
    },
    "linux": {
      "target": "deb",
      "icon": "./res/img/sedas-manager-logo.png"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@types/uuid": "^9.0.8",
    "electron-squirrel-startup": "^1.0.1",
    "leaflet": "^1.9.4",
    "md5": "^2.3.0",
    "node-addon-api": "^8.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron-forge/publisher-github": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^30.0.9",
    "typescript": "^5.7.3"
  }
}
