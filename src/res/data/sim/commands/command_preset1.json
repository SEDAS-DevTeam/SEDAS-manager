        "name": "command preset 1"
    },
    "commands": [
        {
            "input": "<callsign>, fly heading <value>",
            "output": "<callsign>, flying heading <value>",
            "comm": "change-heading"
        },
        {
            "input": "<callsign>, turn right heading <value>",
            "output": "<callsign>, flying heading <value>",
            "comm": "change-heading --right --<value>"
